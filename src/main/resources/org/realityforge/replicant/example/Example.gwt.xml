<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='example'>
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.logging.LoggingDisabled"/>
  <inherits name="org.realityforge.replicant.Replicant"/>
  <inherits name="org.realityforge.gwt.datatypes.Datatypes"/>
  <inherits name="com.google.gwt.inject.Inject"/>
  <inherits name="org.realityforge.gwt.webpoller.WebPoller"/>
  <inherits name="org.realityforge.gwt.lognice.Lognice"/>

  <set-configuration-property name="CssResource.enableGss" value="true" />
  <set-configuration-property name="CssResource.legacy" value="true" />

  <entry-point class='org.realityforge.replicant.example.client.Example'/>

  <source path='client'/>
  <source path='shared'/>
</module>
