<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://xmlns.jcp.org/xml/ns/javaee" bean-discovery-mode="<%= repository.ee.bean_discovery_mode %>" version="1.1">
<% unless repository.ee.cdi_scan_excludes.empty? -%>
  <scan>
<% repository.ee.cdi_scan_excludes.sort.uniq.each do |pattern| -%>
    <exclude name="<%= pattern %>"/>
<% end -%>
  </scan>
<% end -%>
<% repository.ee.beans_xml_content_fragments.each do |fragment| -%>
<%= fragment -%>
<% end -%>
<% repository.ee.resolved_beans_xml_fragments.each do |fragment| -%>
<%= fragment -%>
<% end -%>
</beans>
